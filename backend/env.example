# Backend Service Environment Configuration
# Copy this file to .env and update the values

# Backend Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_URL=http://localhost:8000

# CORS Configuration
FRONTEND_URL=http://localhost:3000

# Environment
PYTHON_ENV=development

# Authentication Configuration
JWT_SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30

# Database Configuration (for future use)
DATABASE_URL=postgresql://user:password@localhost:5432/trip_optimizer

# Redis Configuration (for future use)
REDIS_URL=redis://localhost:6379

# MongoDB Configuration (for future use)
MONGODB_URL=mongodb://localhost:27017/trip_optimizer

# AWS Configuration (for future use)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET=trip-optimizer-documents